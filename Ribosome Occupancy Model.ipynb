    "import argparse\n",
    "import matplotlib.pyplot as plt\n",
    "from ipywidgets import *\n",
    "import ipywidgets as widgets\n",
    "from IPython.display import display\n",
    "plt.style.use('ggplot')\n",
    "#from discreteMarkovChain import markovChain"
   ]
